{
  // editor
  "editor.accessibilitySupport": "off",
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'Courier New', monospace",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.guides.bracketPairs": "active",
  // workbench
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.editor.tabSizing": "fixed",
  // file
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  // etc
  "debug.inlineValues": "on",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // terminal
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font'",
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.suggest.enabled": false,
  // git
  "git.confirmSync": false,
  // go
  "go.useLanguageServer": true,
  "gopls": {
    "formatting.gofumpt": true
  },
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  "go.toolsManagement.autoUpdate": true,
  "go.diagnostic.vulncheck": "Imports",
  // Markdown
  "markdown-preview-github-styles.colorTheme": "light",
  "markdown-preview-enhanced.enablePreviewZenMode": false,
  // drawio
  "hediet.vscode-drawio.resizeImages": null,
  "hediet.vscode-drawio.theme": "atlas",
  // codelens
  "diffEditor.codeLens": true,
  // gcp
  "cloudcode.project": "print-service-024",
  "geminicodeassist.project": "print-service-024",
  "geminicodeassist.displayInlineContextHint": false,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "chat.instructionsFilesLocations": {
    ".github/instructions": true,
    "/var/folders/tv/w8rpspjs7m1dfmjzf3v4rdpw0000gn/T/postman-collections-post-response.instructions.md": true,
    "/var/folders/tv/w8rpspjs7m1dfmjzf3v4rdpw0000gn/T/postman-collections-pre-request.instructions.md": true,
    "/var/folders/tv/w8rpspjs7m1dfmjzf3v4rdpw0000gn/T/postman-folder-post-response.instructions.md": true,
    "/var/folders/tv/w8rpspjs7m1dfmjzf3v4rdpw0000gn/T/postman-folder-pre-request.instructions.md": true,
    "/var/folders/tv/w8rpspjs7m1dfmjzf3v4rdpw0000gn/T/postman-http-request-post-response.instructions.md": true,
    "/var/folders/tv/w8rpspjs7m1dfmjzf3v4rdpw0000gn/T/postman-http-request-pre-request.instructions.md": true
  },
}
